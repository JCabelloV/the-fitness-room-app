<div class="container mt-3">
  <h1>Usuarios</h1>

  <ul>
    <% @users.each do |user| %>
      <li><%=  link_to user.full_name, user_path(user.id) %> - <%= link_to "Editar", edit_user_path(user) %></li>
    
    <% end %>
  </ul>
  <%= link_to "Nuevo usuario", new_user_path, class: "btn btn-primary"%>
</div>

