<h1>Crear usuario</h1>
<%= form_with model: @user do |f| %>
  <div class="field">
    
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="actions">
    <%= f.submit "Crear usuario" %>
  </div>
<% end %>
